{
  "back flip": "xdotool key alt+Tab",
  "be quiet": "espeak 'going silent' &> /dev/null && amixer set Master playback mute",
  "be silent": "espeak 'going silent' &> /dev/null && amixer set Master playback mute",
  "beginning of file": "xdotool key Control+Home",
  "check email": "thunderbird &",
  "clear": "clear",
  "close assistant": "aplay ./mind/boot/sounds/beep_close.wav && pkill -f root_arch &",
  "close email": "wmctrl -c thunderbird",
  "close firefox": "wmctrl -c $BROWSER",
  "close other tabs": "xdotool key Control+Shift+F4 &",
  "close project m": "pkill projectM-pulseaudio",
  "close status": "pkill -f htop &",
  "close tab": "xdotool key Control+w &",
  "close terminal": "wmctrl -c terminal",
  "close window": "xdotool key Control+w",
  "control c": "xdotool key Control+c",
  "copy": "xdotool key Control+c",
  "cut": "xdotool key Control+x",
  "down": "xdotool key Page_Down",
  "echo username": "espeak $USERNAME &> /dev/null | tee /dev/tty",
  "echo hostname": "hostname | tee /dev/tty | espeak &> /dev/null",
  "echo address": "ifconfig | egrep '([0-9]{1,3}\\.){3}[0-9]{1,3}' | tee /dev/tty | espeak 'current eye pee addresses' &> /dev/null",
  "enter": "xdotool key Return",
  "escape": "xdotool key Escape",
  "end": "xdotool key End",
  "end of file": "xdotool key Control+End",
  "find": "xdotool key Control+F",
  "fullscreen": "xdotool key F11",
  "go to firefox": "wmctrl -a firefox &",
  "go to the web": "wmctrl -a $BROWSER &",
  "go to terminal": "wmctrl -a terminal &",
  "hello root": "espeak -s 133 'greetings sir' &> /dev/null",
  "home": "xdotool key Home",
  "invite everyone": "while true; do xdotool click 1; sleep .1; xdotool key Down; xdotool click 1; xdotool mousemove_relative --sync 0 -22; xdotool click 1; sleep .1; xdotool mousemove_relative --sync 0 22; done",
  "list": "xdotool key l && xdotool key s",
  "list access points": "sudo iwlist wlp4s0 scan | less",
  "list block devices": "lsblk -fp",
  "list modules": "lsmod",
  "list network devices": "ip link",
  "list processes": "ps aux | less &",
  "matrix": "cmatrix & espeak -s 133 'there is no spoon...' &> /dev/null &",
  "maximize window": "xdotool key alt+space && sleep .1 && xdotool key x",
  "minimize window": "xdotool key alt+space && sleep .1 && xdotool key n",
  "new file": "xdotool key Control+n &",
  "next tab": "xdotool key Control+Page_Down &",
  "new tab": "xdotool key Control+t &",
  "ok computer": "espeak -s 111 'fitter, happier, more productive, comfortable, not drinking too much, regular exercise at the gym, three days a week' &> /dev/null",
  "open arch wiki": "$BROWSER https://wiki.archlinux.org &",
  "open assistant forum": "$BROWSER http://openassistant.org/community/ &",
  "open commands": "pycharm &",
  "open commands file": "pycharm &",
  "open editor": "subl &",
  "open project em": "projectM-pulseaudio &",
  "open facebook": "$BROWSER https://www.facebook.com &",
  "open firefox": "$BROWSER &",
  "open web": "$BROWSER &",
  "open the web": "$BROWSER &",
  "open status": "$TERMINAL htop && xdotool key alt+space && sleep .1 && xdotool key x &",
  "open stella": "./mind/stella/words/hello.py | $VOICE & ./run.sh -c -H20 -m0 -M mind/stella",
  "open tab": "xdotool key Control+t &",
  "open terminal": "$TERMINAL && wmctrl -a terminal &",
  "open the pod bay doors": "feh -F ./mind/root_arch/images/hal.jpg & aplay ./mind/root_arch/sounds/sorry_dave.wav && sleep .5 && pkill -f feh &",
  "open youtube": "$BROWSER https://www.youtube.com &",
  "page up": "xdotool key Page_Up",
  "page down": "xdotool key Page_Down",
  "paste": "xdotool key Control+v &",
  "play": "xdotool key Shift+A &",
  "previous tab": "xdotool key Control+Page_Up &",
  "print mind directory": "echo $MINDDIR",
  "print working directory": "pwd -P",
  "quit program": "xdotool key Control+q",
  "quit web": "wmctrl -c $BROWSER",
  "quit the web": "wmctrl -c $BROWSER",
  "quit status": "pkill -f htop &",
  "quit thunderbird": "wmctrl -c thunderbird",
  "reboot assistant": "espeak  -s 133 rebooting &> /dev/null & ./run.sh -c -H20 -m1 -M mind/root_arch && pkill -of root_arch",
  "record": "xdotool key R &",
  "red pill": "espeak -s 133 'all delusion is forever temporary...' &> /dev/null & pkill -f cmatrix &",
  "rewind": "xdotool key Home &",
  "root": "espeak -s 133 'yes sir' &> /dev/null",
  "root mind": "espeak -s 133 'yes sir' &> /dev/null",
  "run diagnostics": "./mind/root_arch/words/diagnostics.sh",
  "save document": "xdotool key Control+s && espeak -s 133 saved",
  "save file": "xdotool key Control+s && espeak -s 133 saved",
  "select all": "xdotool key Control+a",
  "scroll down": "xdotool key Page_Down",
  "scroll up": "xdotool key Page_Up",
  "search web": "$BROWSER https://duckduckgo.com && sleep 1 && xdotool key Tab Return &",
  "show status": "$TERMINAL htop && xdotool key alt+space && sleep .1 && xdotool key x &",
  "show useful commands": "cat ~/useful_commands.txt",
  "sing a song": "aplay ./mind/root_arch/sounds/daisy.wav",
  "sing me a song": "aplay ./mind/root_arch/sounds/daisy.wav",
  "sing for me": "aplay ./mind/root_arch/sounds/daisy.wav",
  "suspend machine": "echo suspending machine | $VOICE && systemctl suspend",
  "switch to firefox": "wmctrl -a firefox &",
  "switch to terminal": "wmctrl -a terminal &",
  "system status": "$TERMINAL htop &",
  "talk to me": "amixer set Master playback unmute && espeak 'i am back...' &> /dev/null",
  "tell me about you": "espeak -s 133 'i am the open assistant root mind... my job is to manage operating system control functions... open assistant is composed of three layers of minds... including boot... root... and user minds... the boot mind launches a root mind such as me... i am able to launch user minds such as stella...' &> /dev/null",
  "terminal": "wmctrl -a terminal &",
  "time is it?": "espeak -s 133 'the time is' &> /dev/null && date +%l:%M | tee /dev/tty | espeak -s 133 &> /dev/null",
  "thanks root": "aplay ./mind/root_arch/sounds/play_a_game.wav",
  "turn on wireless": "sudo ip link set wlp4s0 up",
  "undo": "xdotool key Control+z",
  "voice note": "audacity &",
  "volume down": "pamixer --decrease 10",
  "volume mute": "espeak 'going silent' &> /dev/null && amixer set Master playback mute",
  "volume up": "pamixer --increase 10",
  "what day?": "espeak -s 133 'today is' &> /dev/null && date +%A | tee /dev/tty | espeak -s 133 &> /dev/null",
  "what day is?": "espeak -s 133 'today is' &> /dev/null && date +%A | tee /dev/tty | espeak -s 133 &> /dev/null",
  "what day is it?": "date +%A | tee /dev/tty | espeak -s 133 &> /dev/null",
  "what day is it today?": "espeak -s 133 'today is' &> /dev/null && date +%A | tee /dev/tty | espeak -s 133 &> /dev/null",
  "what is the time?": "saytime | espeak -s 133",
  "what is the weather today?": "./mind/stella/words/weather.py | tee /dev/tty |  espeak -s 133 &> /dev/null",
  "what is the weather?": "./mind/stella/words/weather.py | tee /dev/tty |  espeak -s 133 &> /dev/null",
  "what time?": "espeak -s 133 'the time is' &> /dev/null && date +%l:%M | tee /dev/tty | espeak -s 133 &> /dev/null",
  "what time is?": "espeak -s 133 'the time is' &> /dev/null && date +%l:%M | tee /dev/tty | espeak -s 133 &> /dev/null",
  "what time is it?": "espeak -s 133 'the time is' &> /dev/null && date +%l:%M | tee /dev/tty | espeak -s 133 &> /dev/null"
}
